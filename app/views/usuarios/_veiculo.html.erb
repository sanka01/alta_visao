<div class="container center">
  <div class="card">
    <div class="card-body">
      <div class="col-md-6">

        <strong>Modelo:</strong>
        <p><%= veiculo.modelo %></p>

      </div>
      <div class="col-md-6">

        <strong>Ano: </strong>
        <p><%= veiculo.ano %></p>
      </div>
      <div class="col-md-6">

        <strong>Renavan: </strong>
        <p><%= veiculo.renavan %></p>
      </div>
      <div class="col-md-6">

        <strong>Chassi: </strong>
        <p><%= veiculo.chassi %></p>
      </div>
      <div class="col-md-6">

        <strong>Placa: </strong>
        <p><%= veiculo.placa %></p>
      </div>
    </div>
    <div class="col-md-4">

    <%= link_to t(:show), veiculo_path(veiculo), :class => "btn btn-info" %>
    </div>
  </div>
</div>