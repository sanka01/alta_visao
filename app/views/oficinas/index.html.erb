<p id="notice"><%= notice %></p>

<h1>Oficinas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Usuario</th>
      <th>Endereco</th>
      <th>Franquia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oficinas.each do |oficina| %>
      <tr>
        <td><%= oficina.nome %></td>
        <td><%= oficina.usuario %></td>
        <td><%= oficina.endereco %></td>
        <td><%= oficina.franquia %></td>
        <td><%= link_to 'Show', oficina %></td>
        <td><%= link_to 'Edit', edit_oficina_path(oficina) %></td>
        <td><%= link_to 'Destroy', oficina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oficina', new_oficina_path %>
