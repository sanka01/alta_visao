<p id="notice"><%= notice %></p>

<h1>Contas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Nivel permissao</th>
      <th>Usuario</th>
      <th>Oficina</th>
      <th>Franquia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contas.each do |conta| %>
      <tr>
        <td><%= conta.nome %></td>
        <td><%= conta.nivel_permissao %></td>
        <td><%= conta.usuario %></td>
        <td><%= conta.oficina %></td>
        <td><%= conta.franquia %></td>
        <td><%= link_to 'Show', conta %></td>
        <td><%= link_to 'Edit', edit_conta_path(conta) %></td>
        <td><%= link_to 'Destroy', conta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conta', new_conta_path %>
